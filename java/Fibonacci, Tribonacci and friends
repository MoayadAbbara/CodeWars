import java.util.Arrays;
public class Xbonacci {
    public double[] xbonacci(double[] signature, int n) {
        if (n <= signature.length)
          return Arrays.copyOf(signature, n);
        double[] arr = new double[n];
        int len = signature.length;
        for(int i = 0 ; i < len ; i++)
            arr[i] = signature[i];
        for (int i = len ; i < n ; i++)
            for(int j = 1 ; j <= len ; j++)
                arr[i] += arr[i-j];
        return arr;
    }
}
