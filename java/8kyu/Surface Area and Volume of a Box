/*
DESCRIPTION:
Write a function that returns the total surface area and volume of a box as an array: [area, volume]
*/
public class Kata {
    public static int[] getSize(int w,int h,int d) {
        int[] result = new int[2];
        result[0] = 2 * (w*h + h*d + w*d);
        result[1] = w * h * d;
        return result;
    }
}
